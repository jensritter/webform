{
  "name": "webform-js",
  "version": "1.0.0",
  "description": "required JS for html page",
  "repository": {
    "type": "git",
    "url": "git@github.com:jensritter/webforms.git"
  },
  "author": "Jens Ritter<jens.ritter@gmail.com>",
  "license": "ISC",
  "scripts": {
    "test": "jest"
  },
  "jest": {
    "roots": [
      ".",
      "src/test/js"
    ],
    "setupFiles": [
      "<rootDir>/src/test/setup/jest-config-fail-always.js"
    ],
    "testResultsProcessor": "jest-junit",
    "reporters": [
      "default",
      "jest-junit"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "text",
      "cobertura"
    ],
    "coverageDirectory": "target/coverage"
  },
  "jest-junit": {
    "outputDirectory": "target/surefire-reports",
    "outputName": "jest-junit.xml"
  },
  "devDependencies": {
    "express": "^4.17.1",
    "jest": "^27.1.0",
    "jest-junit": "^13.0.0",
    "node-fetch": "^2.6.1"
  },
  "dependencies": {
    "jsonform": "^2.2.4",
    "jquery": "^3.6.0",
    "underscore": "^1.13.1",
    "JSV": "^4.0.2"
  }
}
